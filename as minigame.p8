version 8
__lua__
-- animation system	
-- 22.11.17
-- viktoria lierhaus
-- v1


function _init()
 frame				 = 1
 fps						 = 30
 sec						 = 0
 
-- x								 = 15
-- animframe = 0
	
	player = {
  x = 64,
  y = 64,
  frames = {
  { 1,  2,  3, 2}, -- 1 = left
  {17, 18, 19, 18},	-- 2 = right
  {33, 34, 35, 34}, -- 3 = up
  {49, 50, 51, 50}		-- 4 = down
  },
 	findex = 1,
 	ffreq 	= 10,
 	astate = 3
 }	

	cam = {
		x = 64,
		y = 64,
		x_min = 64,
		y_min = 64
	}

end


function _update()
 
 if (btn(0)) then
 	player.x -= 1
 	player.astate = 0
 elseif (btn(1)) then
 	player.x += 1
 	player.astate = 1
 end
 
 if (btn(2)) then
 	player.y -= 1
 	player.astate = 2
 elseif (btn(3)) then
 	player.y += 1
 	player.astate = 3
 end
	
	--thing
	if (btnp(4)) then
		mset(
		player.x/8,
		player.y/8,
		5
		)
	end
	
	if (btnp(5)) then
		mset(
		player.x/8,
		player.y/8,
		0
		)
	
	end
	-- player animation
	if (frame%player.ffreq == 0) then
		-- move frame index
		player.findex += 1
		
		-- keep index in bounds
		if (player.findex > #player.frames[1]) then
			player.findex = 1
		end
	end

	
	-- camera motion
	cam.x = player.x
	cam.y = player.y
	
	if (cam.x < cam.x_min) then
		cam.x = cam.x_min
	end
	
		if (cam.y < cam.y_min) then
		cam.y = cam.y_min
	end
	
	-- frame counter
	frame += 1
	-- sec = flr(frame/fps)
		if (frame%fps == 0) then
		sec += 1
		end
	
end


function _draw()
 cls()

	rectfill(0, 0, 127, 127, 5)
	
	map(		0,	 0,
							0,	 0,
					128, 64
				)
	
	--[[
 print("frame "..frame)
 print("sec: "..sec)
 print("mod(%): "..frame%fps)
	]]--
	--player
	spr(
	player.frames[player.astate+1][player.findex],
	player.x,
	player.y
	)
	
	-- camera
	camera(cam.x - 64, cam.y - 64)
	
	print(
		mget(player.x/8, player.y/8),
		player.x + 10,
		player.y,
		14
	)
	
end
